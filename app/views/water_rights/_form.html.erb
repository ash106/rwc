<%= simple_form_for(@water_right) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :number %>
    <%= f.input :flow_cfs %>
    <%= f.input :flow_ac_ft %>
    <%= f.input :sole_supply_acres %>
    <%= f.input :place_of_use %>
    <%= f.input :change_application_number %>
    <%= f.input :proof_due_date %>
  </div>

  <div class="form-actions text-center">
    <%= f.button :submit %>
  </div>
<% end %>
