<h1 class="text-center">Water Rights</h1>

<div class="table-responsive">
  <table class="table">
    <thead>
      <tr>
        <th>Number</th>
        <th>Flow cfs</th>
        <th>Flow ac ft</th>
        <th>Sole supply acres</th>
        <th>Place of use</th>
        <th>Change application number</th>
        <th>Proof due date</th>
        <th>Priority date</th>
        <th>External link</th>
        <th>User</th>
        <th>Comments</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <%= render(@water_rights) %>
    </tbody>
  </table>
</div>

<div class="text-center">
  <%= link_to 'New Water Right', new_water_right_path, class: 'btn btn-primary' %>
</div>
